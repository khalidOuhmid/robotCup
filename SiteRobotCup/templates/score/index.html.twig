{% extends 'base.html.twig' %}

{% block title %}Hello ScoreController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<h1>Import scores</h1>
<form action="/import-scores" method="post" enctype="multipart/form-data">
    <label for="scores_file">Upload Scores JSON:</label>
    <input type="file" name="scores_file" id="scores_file">
    <button type="submit">Upload</button>
</form>
{% endblock %}
